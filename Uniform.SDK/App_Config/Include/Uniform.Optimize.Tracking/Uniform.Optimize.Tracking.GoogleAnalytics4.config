<configuration
  xmlns:patch="http://www.sitecore.net/xmlconfig/">

  <sitecore>
    <pipelines>
      <group name="uniform" groupName="uniform">
        <pipelines>
          <getTrackerDispatchDestinations>
            <processor type="Uniform.Optimize.Pipelines.GetTrackerDispatchDestinations.GetDestinations, Uniform.Optimize.Tracking.GoogleAnalytics4" />
          </getTrackerDispatchDestinations>
          <getTrackerScriptUrls>
            <processor type="Uniform.Optimize.Pipelines.GetTrackerScriptUrls.GetGaScriptUrl, Uniform.Optimize.Tracking.GoogleAnalytics4" />
          </getTrackerScriptUrls>
        </pipelines>
      </group>
    </pipelines>
  </sitecore>
</configuration>
